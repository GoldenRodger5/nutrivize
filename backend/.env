# Production environment variables
# Note: For local development, use .env.local instead
ENVIRONMENT=production

# Database - Update these values in Render environment settings
# IMPORTANT: Don't store actual credentials here
MONGODB_URL=

# Firebase
FIREBASE_SERVICE_ACCOUNT_PATH=

# API Keys - Set these in Render environment settings
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_CLOUD_VISION_CREDENTIALS=
GOOGLE_APPLICATION_CREDENTIALS=

# CORS Settings
FRONTEND_URL=https://nutrivize.onrender.com
ADDITIONAL_CORS_ORIGINS=https://nutrivize-frontend.onrender.com,https://nutrivize.onrender.com

# Security
SECRET_KEY=nutrivize-v2-production-secret-key
