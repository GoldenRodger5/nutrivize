# Environment Configuration
ENVIRONMENT=development

# Database
MONGODB_URL=mongodb+srv://username:password@cluster.mongodb.net/nutrivize_v2?retryWrites=true&w=majority

# Firebase
FIREBASE_SERVICE_ACCOUNT_PATH=path/to/firebase-service-account.json

